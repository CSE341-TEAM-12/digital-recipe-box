{
  "swagger": "2.0",
  "info": {
    "title": "Digital Recipe Box API",
    "description": "Digital Recipe Box API",
    "version": "1.0.0",
    "contact": {
      "name": "CSE341 Team-12",
      "url": "https://github.com/CSE341-TEAM-12/digital-recipe-box"
    }
  },
  "host": "localhost:3000",
  "basePath": "/",
  "tags": [
    {
      "name": "General",
      "description": "General API endpoints and information"
    },
    {
      "name": "Recipes",
      "description": "Recipe management endpoints - Create, read, and manage cooking recipes with ingredients, instructions, cooking time, and serving information"
    },
    {
      "name": "Cookbooks",
      "description": "Cookbook management endpoints - Organize recipes into collections for better management and organization"
    },
    {
      "name": "Reviews",
      "description": "Recipe review endpoints - Rate and review recipes to help other users discover great recipes"
    }
  ],
  "schemes": [
    "http",
    "https"
  ],
  "securityDefinitions": {
    "bearerAuth": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "description": "JWT token for authentication. Format: Bearer {token}"
    }
  },
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/": {
      "get": {
        "tags": [
          "General"
        ],
        "summary": "API Welcome Message",
        "description": "Returns a welcome message for the Digital Recipe Box API. This endpoint can be used to check if the API is running and accessible.",
        "responses": {
          "200": {
            "description": "Welcome message returned successfully",
            "schema": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "example": "string"
                },
                "example": {
                  "type": "string",
                  "example": "Welcome to Digital Recipe Box API"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
    "/recipes/": {
      "get": {
        "tags": [
          "Recipes"
        ],
        "summary": "Get all public recipes",
        "description": "Retrieve a list of all public recipes available to all users. This endpoint returns recipes that have been marked as public by their creators.",
        "responses": {
          "200": {
            "description": "List of recipes retrieved successfully",
            "schema": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "example": "array"
                },
                "items": {
                  "$ref": "#/definitions/Recipe"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      },
      "post": {
        "tags": [
          "Recipes"
        ],
        "summary": "Create a new recipe",
        "description": "Create a new recipe with ingredients, instructions, and metadata. Requires authentication. The recipe will be associated with the authenticated user.",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "description": "Recipe data",
            "required": true,
            "schema": {
              "$ref": "#/definitions/RecipeInput"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Recipe created successfully",
            "schema": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "example": "object"
                },
                "properties": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "example": "string"
                        },
                        "example": {
                          "type": "string",
                          "example": "Recipe created successfully"
                        }
                      }
                    },
                    "recipe": {
                      "$ref": "#/definitions/Recipe"
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "Invalid input data or validation error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "401": {
            "description": "Authentication required",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "500": {
            "description": "Internal server error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    },
    "/recipes/{id}": {
      "get": {
        "tags": [
          "Recipes"
        ],
        "summary": "Get a recipe by ID",
        "description": "Retrieve a specific recipe by its unique ID. Access depends on recipe visibility - public recipes can be accessed by anyone, private recipes require authentication and ownership.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Unique identifier of the recipe",
            "example": "60f7b1b3b3f3b3b3b3f3b3b3"
          }
        ],
        "responses": {
          "200": {
            "description": "Recipe retrieved successfully",
            "schema": {
              "$ref": "#/definitions/Recipe"
            }
          },
          "403": {
            "description": "Access forbidden - private recipe",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "404": {
            "description": "Recipe not found",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "500": {
            "description": "Internal server error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    },
    "/cookbooks/": {
      "get": {
        "tags": [
          "Cookbooks"
        ],
        "summary": "Get user cookbooks",
        "description": "Retrieve all cookbooks owned by the authenticated user. This endpoint returns both public and private cookbooks created by the user.",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "List of user cookbooks retrieved successfully",
            "schema": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "example": "array"
                },
                "items": {
                  "$ref": "#/definitions/Cookbook"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "401": {
            "description": "Authentication required",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "500": {
            "description": "Internal server error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      },
      "post": {
        "tags": [
          "Cookbooks"
        ],
        "summary": "Create a new cookbook",
        "description": "Create a new cookbook to organize recipes. Requires authentication. The cookbook will be associated with the authenticated user and can contain multiple recipes.",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "description": "Cookbook data",
            "required": true,
            "schema": {
              "$ref": "#/definitions/CookbookInput"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Cookbook created successfully",
            "schema": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "example": "object"
                },
                "properties": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "example": "string"
                        },
                        "example": {
                          "type": "string",
                          "example": "Cookbook created successfully"
                        }
                      }
                    },
                    "cookbook": {
                      "$ref": "#/definitions/Cookbook"
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "Invalid input data or validation error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "401": {
            "description": "Authentication required",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "500": {
            "description": "Internal server error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    },
    "/cookbooks/{id}": {
      "get": {
        "tags": [
          "Cookbooks"
        ],
        "summary": "Get cookbook by ID",
        "description": "Retrieve a specific cookbook by its ID. Only the owner of the cookbook can access it. Returns the cookbook with all associated recipe information.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Unique identifier of the cookbook",
            "example": "60f7b1b3b3f3b3b3b3f3b3b3"
          },
          {
            "name": "authorization",
            "in": "header",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Cookbook retrieved successfully",
            "schema": {
              "$ref": "#/definitions/Cookbook"
            }
          },
          "401": {
            "description": "Authentication required",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "403": {
            "description": "Access forbidden - not the owner",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "404": {
            "description": "Cookbook not found",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "500": {
            "description": "Internal server error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    },
    "/reviews/recipe/{recipeId}": {
      "get": {
        "tags": [
          "Reviews"
        ],
        "summary": "Get reviews for a recipe",
        "description": "Retrieve all reviews for a specific recipe by recipe ID. Returns ratings and comments from users who have reviewed this recipe.",
        "parameters": [
          {
            "name": "recipeId",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Unique identifier of the recipe to get reviews for",
            "example": "60f7b1b3b3f3b3b3b3f3b3b3"
          }
        ],
        "responses": {
          "200": {
            "description": "Reviews retrieved successfully",
            "schema": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "example": "array"
                },
                "items": {
                  "$ref": "#/definitions/Review"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "403": {
            "description": "Access forbidden - private recipe",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "404": {
            "description": "Recipe not found",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "500": {
            "description": "Internal server error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      },
      "post": {
        "tags": [
          "Reviews"
        ],
        "summary": "Add a review to a recipe",
        "description": "Add a new review (rating and optional comment) to a specific recipe. Requires authentication. Users can rate recipes from 1 to 5 stars and provide feedback.",
        "parameters": [
          {
            "name": "recipeId",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Unique identifier of the recipe to review",
            "example": "60f7b1b3b3f3b3b3b3f3b3b3"
          },
          {
            "name": "authorization",
            "in": "header",
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "description": "Review data (rating and optional comment)",
            "required": true,
            "schema": {
              "$ref": "#/definitions/ReviewInput"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Review created successfully",
            "schema": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "example": "object"
                },
                "properties": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "example": "string"
                        },
                        "example": {
                          "type": "string",
                          "example": "Review created successfully"
                        }
                      }
                    },
                    "review": {
                      "$ref": "#/definitions/Review"
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "Invalid input data or validation error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "401": {
            "description": "Authentication required",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Recipe not found",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "409": {
            "description": "Conflict"
          },
          "500": {
            "description": "Internal server error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    }
